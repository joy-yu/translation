{
    "name": "Translate Extension",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "A translate tool for google chrome extension",


    "content_scripts": [{
        "matches": [
            "http://*/*",
            "https://*/*"
        ],
        "js": [
            "jquery.min.js",
            "translate.js"
        ]
    }]


}